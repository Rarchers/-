<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rarcher">

    <!-- 各种奇怪的权限 -->
    <!-- 指纹识别 -->
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="com.fingerprints.service.ACCESS_FINGERPRINT_MANAGER" />
    <uses-permission android:name="com.samsung.android.providers.context.permission.WRITE_USE_APP_FEATURE_SURVEY" />
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 从SDCard读取数据权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!-- 往SDCard中写入数据的权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 修改系统权限 -->
    <uses-permission
        android:name="android.permission.WRITE_SETTINGS"
        android:required="true" />
    <!-- 震动 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <!-- 开机启动 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pics"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pics"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Acticitys.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Register.LoginActivity" />
        <activity android:name=".Login_Register.RegisterActivity" />
        <activity android:name=".Login_Register.Register_two" />
        <activity android:name=".Login_Register.ForgetPwdActivity" />
        <activity android:name=".Login_Register.ForgetPwd_two" />
        <activity android:name=".Acticitys.MainActivity"
            android:windowSoftInputMode="adjustResize|stateHidden">
        </activity>
        <activity android:name=".Acticitys.Setting_ME" />
        <activity android:name=".Acticitys.AllOders" />
        <activity
            android:name=".Acticitys.Oders"
            android:label="@string/title_activity_oders"
            android:theme="@style/AppTheme" />
        <activity android:name=".Acticitys.FigureSafe" />
        <activity android:name=".Acticitys.SafeNote" />
        <activity android:name=".Acticitys.Adding" />
        <activity
            android:name=".Acticitys.Details"
            android:label="@string/title_activity_details"
            android:theme="@style/AppTheme" />
        <activity android:name=".Acticitys.List_Been" />
        <activity android:name=".Acticitys.LifeArrange"></activity>
    </application>

</manifest>